{
  "name": "linalg-monorepo",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "build": "pnpm --recursive --filter \"@linalg/demo-*\" build",
    "build:vectors": "pnpm --filter @linalg/demo-vectors build",
    "build:matrix": "pnpm --filter @linalg/demo-matrix-transforms build",
    "dev:vectors": "pnpm --filter @linalg/demo-vectors dev",
    "dev:matrix": "pnpm --filter @linalg/demo-matrix-transforms dev",
    "typecheck": "pnpm --recursive --filter \"@linalg/demo-*\" exec tsc --noEmit"
  },
  "volta": {
    "node": "25.6.0"
  }
}
